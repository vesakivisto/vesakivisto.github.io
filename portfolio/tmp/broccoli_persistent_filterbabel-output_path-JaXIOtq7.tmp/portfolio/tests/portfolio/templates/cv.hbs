<div class="container">
  <div class="row">
    <div class="twelve column">
      <h5>Personal info</h5>
      <hr>
      {{#each model.personalInfos as |info|}}
        <p>{{info.title}} {{info.description}}</p>
      {{/each}}

      <h5>Work experience</h5>
      <hr>
      {{#each model.workExperiences as |experience|}}
        <h6>{{experience.title}}</h6>
        {{#each experience.work as |work|}}
          <p>{{work.duration}}</p>
          <p>{{work.description}}</p>
        {{/each}}
        <p>{{experience.duration}}</p>
        <p>{{experience.description}}</p>
      {{/each}}

      <h5>Education</h5>
      <hr>
      {{#each model.educations as |education|}}
        <h6>{{education.title}}</h6>
        <p>{{education.school}}</p>
        <p>{{education.duration}}</p>
        <p>{{education.description}}</p>
      {{/each}}

      <h5>Languages</h5>
      <hr>
      {{#each model.languages as |language|}}
        <p>{{language.title}} {{language.level}}</p>
        {{#each language.notes as |note|}}
          <p>{{note.text}}</p>
        {{/each}}
      {{/each}}

      <h5>Technical skills</h5>
      <hr>
      {{#each model.skills as |skill|}}
        {{#each skill.items as |item|}}
          <p>{{item.title}}</p>
        {{/each}}
      {{/each}}
    </div>
  </div>
</div>
